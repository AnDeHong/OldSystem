<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.mapper.DishOrderMapper">
    <!-- 查询用户信息 -->
    <select id="getPackDishByNo" resultType="com.example.entity.DishOrder">
        select * from dish_order where delete_mark = 'N' and id = #{dishNo}
    </select>
</mapper>